@page "/department/organization_chart"
@attribute [Authorize]
@inject Manager manager

<OrgChartEmployee Employees="Employees"></OrgChartEmployee>

@code {
    public List<Employee> Employees { get; set; } = [];

    protected override void OnInitialized()
    {
        Employees = manager.EmployeeDB.GetAllActiveAsync().Result;
        base.OnInitialized();
    }
}
